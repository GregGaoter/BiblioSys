package com.dsi.bibliosys.biblioback.mapper;

import org.modelmapper.ModelMapper;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.dsi.bibliosys.biblioback.data.dto.UsagerDto;
import com.dsi.bibliosys.biblioback.data.entity.Usager;

@Service
public class UsagerMapper implements Mapper<Usager, UsagerDto> {

	@Autowired
	private ModelMapper modelMapper;

	@Override
	public UsagerDto mapToDto(Usager source) {
		return modelMapper.map(source, UsagerDto.class);
	}

	@Override
	public Usager mapToEntity(UsagerDto source) {
		return null;
	}

}
