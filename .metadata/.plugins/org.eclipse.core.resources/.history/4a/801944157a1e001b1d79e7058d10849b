package com.dsi.bibliosys.biblioback.service.mapper;

import org.modelmapper.ModelMapper;
import org.springframework.stereotype.Service;

import com.dsi.bibliosys.biblioback.data.entity.Usager;
import com.dsi.bibliosys.biblioback.dto.UsagerDto;

@Service
public class UsagerMapperTest {

	private ModelMapper modelMapper = new ModelMapper();
	private UsagerDto usagerDto = new UsagerDto();

	public UsagerDto mapToDto(Usager usager) {
		modelMapper.map(usager, usagerDto);
		return usagerDto;
	}

}
